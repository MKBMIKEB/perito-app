{
  "name": "perito-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=4096' expo start",
    "start:dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "cross-env NODE_OPTIONS='--max-old-space-size=4096' expo start --web",
    "eject": "expo eject",
    "build:android": "eas build --platform android --profile preview",
    "build:firebase": "eas build --platform android --profile firebase-dist",
    "build:production": "eas build --platform android --profile production",
    "build:local": "npx expo prebuild --platform android --clean && cd android && gradlew assembleRelease",
    "prebuild": "npx expo prebuild --platform android --clean",
    "deploy:firebase": "node scripts/firebase-deploy.js",
    "check:setup": "node scripts/check-setup.js",
    "setup:complete": "npm run check:setup && echo 'âœ… Setup verification complete!'",
    "fix-errors": "fix-errors.bat",
    "clean": "npx react-native start --reset-cache"
  },
